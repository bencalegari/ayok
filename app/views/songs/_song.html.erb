<div id="<%= dom_id song %>" class="w-full sm:w-auto my-5 space-y-5">
  <div>
    <strong class="font-medium mb-1">Title:</strong>
    <%= song.title %>
  </div>
  <div>
    <strong class="font-medium mb-1">Artist:</strong>
    <%= song.artist %>
  </div>
  <div>
    <strong class="font-medium mb-1">Youtube url:</strong>
    <%= song.youtube_url %>
  </div>
</div>
